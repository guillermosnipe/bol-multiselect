{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "bol",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "bol",
            "kebab-case"
        ],
        "variable-name": [true,
          "check-format",
          "allow-leading-underscore"
      ]
    }
}
